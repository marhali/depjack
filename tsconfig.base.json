{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["esnext"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "@depjack/core/*": ["./packages/core/src/*"],
      "@depjack/react/*": ["./packages/react/src/*"]
    },
    "strict": true,
    "declaration": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
